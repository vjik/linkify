{
    "name": "vjik/linkify",
    "type": "library",
    "description": "Matches things like email addresses, web URLs, etc. in the text and makes them clickable links.",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/html": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.7",
        "vimeo/psalm": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Vjik\\Linkify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vjik\\Linkify\\Tests\\": "tests"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true
    }
}
